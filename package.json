    "bootstrap": "^4.5.3",
    "buffer": "^6.0.3",
    "cookie-parser": "^1.4.5",
    "crypto-browserify": "^3.12.0",
    "express": "^4.17.1",
    "global": "^4.4.0",
    "node-fetch": "^2.6.1",
